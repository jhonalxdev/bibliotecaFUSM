	<parameter name="Fecha" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     USUARIO.`CARRERA`,
     PRESTAMOBEAM_A.`SERIALBEAM`,
     PRESTAMOBEAM_A.`ENTREGAPRESTAMO`,
     PRESTAMOBEAM_A.`FECHAPRESTAMO`,
     PRESTAMOBEAM_A.`IDUSUARIO`,
     PRESTAMOBEAM_A.`NOMBREUSUARIO`,
     PRESTAMOBEAM_A.`TIPOUSUARIO`,
     PRESTAMOBEAM_A.`ESTADOPRESTAMO`,
     VIDEOBEAM.`PROYECTORNUM` AS VIDEOBEAM_PROYECTORNUM
FROM
     `USUARIO` USUARIO INNER JOIN `PRESTAMOBEAM` PRESTAMOBEAM ON USUARIO.`IDENTIFICACION` = PRESTAMOBEAM.`IDUSUARIO`,
     `VIDEOBEAM` VIDEOBEAM INNER JOIN `PRESTAMOBEAM` PRESTAMOBEAM_A ON VIDEOBEAM.`SERIALNUM` = PRESTAMOBEAM_A.`SERIALBEAM`
WHERE
     PRESTAMOBEAM.`ENTREGAPRESTAMO` LIKE $P{Fecha}
 AND PRESTAMOBEAM.`FECHAPRESTAMO` LIKE $P{Fecha}
  OR PRESTAMOBEAM.`FECHAPRESTAMO` LIKE "%---%"
ORDER BY
     USUARIO.CARRERA ASC]]>
	</queryString>
